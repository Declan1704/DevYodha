# Prisma (use Prisma Accelerate URL for serverless; direct URL for migrations)
DATABASE_URL="prisma://accelerate.prisma-data.net/?api_key=YOUR_PRISMA_ACCELERATE_API_KEY"
DIRECT_URL="postgresql://user:password@host:5432/dbname?sslmode=require"

# JWT secret for auth (use a strong secret in production)
JWT_SECRET="your-secure-jwt-secret"
